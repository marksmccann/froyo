{
    "name": "testing-library-froyojs",
    "version": "0.3.2",
    "description": "A package that extends DOM Testing Library to work with Froyo",
    "main": "dist/lib/index.js",
    "module": "dist/es/index.js",
    "scripts": {
        "build": "run-s clean build:*",
        "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/lib",
        "build:es": "cross-env BABEL_ENV=es babel src --out-dir dist/es",
        "clean": "rimraf dist"
    },
    "author": "Mark McCann <marksmccann@gmail.com>",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/marksmccann/froyo"
    },
    "keywords": [
        "tests",
        "testing-library",
        "froyojs",
        "froyo"
    ],
    "peerDependencies": {
        "froyojs": "*"
    },
    "dependencies": {
        "@testing-library/dom": "^8.20.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "cross-env": "^7.0.3",
        "froyojs": "^0.4.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^4.1.0"
    }
}
