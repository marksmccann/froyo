{
    "name": "testing-library-froyojs",
    "version": "1.0.0-alpha.2",
    "description": "A package that extends DOM Testing Library to work with Froyo",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "prebuild": "npm run clean",
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "clean": "rm -rf dist"
    },
    "author": "Mark McCann <marksmccann@gmail.com>",
    "license": "MIT",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/marksmccann/froyo/tree/main/packages/testing-library-froyojs"
    },
    "keywords": [
        "tests",
        "testing-library",
        "froyojs",
        "froyo",
        "ice cream",
        "frozen yogurt"
    ],
    "peerDependencies": {
        "froyojs": "^1.0.0-alpha.0"
    },
    "dependencies": {
        "@testing-library/dom": "^8.20.0"
    },
    "devDependencies": {
        "froyojs": "^1.0.0-alpha.1",
        "tsup": "^6.7.0",
        "typescript": "^4.9.5"
    }
}
