{
    "name": "testing-library-froyojs",
    "version": "0.5.1",
    "description": "A package that extends DOM Testing Library to work with Froyo",
    "main": "dist/lib/index.js",
    "module": "dist/es/index.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "prebuild": "npm run clean",
        "build": "rollup -c --bundleConfigAsCjs",
        "postbuild": "cpy dist/es/types dist/types --flat && rimraf dist/es/types",
        "clean": "rimraf dist"
    },
    "author": "Mark McCann <marksmccann@gmail.com>",
    "license": "MIT",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/marksmccann/froyo/tree/main/packages/testing-library-froyojs"
    },
    "keywords": [
        "tests",
        "testing-library",
        "froyojs",
        "froyo",
        "ice cream",
        "frozen yogurt"
    ],
    "peerDependencies": {
        "froyojs": "^0.6.1"
    },
    "dependencies": {
        "@testing-library/dom": "^8.20.0"
    },
    "devDependencies": {
        "cpy-cli": "^4.2.0",
        "cross-env": "^7.0.3",
        "froyojs": "^0.7.1",
        "rimraf": "^4.1.0",
        "rollup": "^3.17.2",
        "typescript": "^4.9.5"
    }
}
