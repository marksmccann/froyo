{
    "name": "froyojs",
    "version": "0.7.2",
    "description": "A framework for building UI components with Vanilla JavaScript",
    "main": "dist/froyojs.js",
    "module": "dist/froyojs.mjs",
    "types": "dist/froyojs.d.ts",
    "exports": {
        "import": "./dist/froyojs.mjs",
        "require": "./dist/froyojs.js",
        "types": "./dist/froyojs.d.ts"
    },
    "scripts": {
        "prebuild": "npm run clean",
        "build": "rollup -c --bundleConfigAsCjs",
        "postbuild": "rm -rf dist/dts",
        "clean": "rm -rf dist"
    },
    "author": "Mark McCann <marksmccann@gmail.com>",
    "license": "MIT",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/marksmccann/froyo/tree/main/packages/froyojs"
    },
    "keywords": [
        "vanilla",
        "framework",
        "ui",
        "component",
        "froyo",
        "ice cream",
        "frozen yogurt"
    ],
    "dependencies": {
        "@types/prop-types": "^15.7.5",
        "prop-types": "^15.8.1"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-terser": "^0.3.0",
        "@rollup/plugin-typescript": "^11.0.0",
        "rollup": "^3.10.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-dts": "^5.2.0",
        "typescript": "^4.9.5"
    }
}
