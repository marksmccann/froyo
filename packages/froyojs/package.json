{
    "name": "froyojs",
    "version": "0.5.2",
    "description": "An ultra lightweight framework for building UI components",
    "main": "dist/froyojs.js",
    "module": "dist/froyojs.mjs",
    "scripts": {
        "prebuild": "npm run clean",
        "build": "rollup -c --bundleConfigAsCjs",
        "clean": "rm -rf dist"
